    {
      "title": "HTTP Requests / Sec",
      "type": "timeseries",
      "gridPos": { "h": 10, "w": 8, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum by (method, path) (rate(http_requests_total{path!='/v1/prometheus/'}[1m]))",
          "legendFormat": "{{method}} {{path}}"
        }
      ]
    },
    {
      "title": "HTTP Latency (P50, P95, P99)",
      "type": "timeseries",
      "gridPos": { "h": 10, "w": 8, "x": 8, "y": 1 },
